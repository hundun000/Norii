                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_2xM4UF4IEemVPufykw3Hxg" name="hide" specification="_2xLqMF4IEemVPufykw3Hxg">
                  <language>JAVA</language>
                  <body>&#xD;
    keys.put(Keys.LEFT, false);&#xD;
    keys.put(Keys.RIGHT, false);&#xD;
