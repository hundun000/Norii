                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_76U1gIIUEeqnLuUlSLWUKw" name="BattleMap" specification="_76QkEIIUEeqnLuUlSLWUKw">
                  <language>JAVA</language>
                  <body>&#xD;
    super(MapFactory.MapType.BATTLE_MAP, mapPath);&#xD;
    if (!Utility.isAssetLoaded(mapPath)) {&#xD;
        Gdx.app.debug(TAG, &quot;Map not loaded&quot;);&#xD;
        return;&#xD;
    }&#xD;
    collisionLayer = currentMap.getLayers().get(MAP_COLLISION_LAYER);&#xD;
    if (collisionLayer == null) {&#xD;
        Gdx.app.debug(TAG, &quot;No collision layer!&quot;);&#xD;
    }&#xD;
    initializeClassVariables();&#xD;
</body>
