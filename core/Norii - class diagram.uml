                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_8H_eEYIUEeqnLuUlSLWUKw" name="initializeUnits" specification="_8H7MoIIUEeqnLuUlSLWUKw">
                  <language>JAVA</language>
                  <body>&#xD;
    final int index = ScreenManager.ScreenParams.ARRAYLIST_OF_OWNERS.ordinal();&#xD;
    if (params[index] != null) {&#xD;
        players = (ArrayList&lt;TeamLeader>) params[index];&#xD;
    } else {&#xD;
        players = new ArrayList&lt;TeamLeader>();&#xD;
    }&#xD;
    for (final TeamLeader player : players) {&#xD;
        for (final Entity unit : player.getTeam()) {&#xD;
            unit.addEntityObserver(this);&#xD;
        }&#xD;
    }&#xD;
</body>
                </ownedBehavior>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_8IBTQIIUEeqnLuUlSLWUKw" name="initializeObservers" specification="_8IAsMIIUEeqnLuUlSLWUKw">
