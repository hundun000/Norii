    for (final TiledMapActor[] tmpa : currentMap.getTiledMapStage().getTiledMapActors()) {&#xD;
        for (final TiledMapActor actor : tmpa) {&#xD;
            actor.addTilemapObserver(this);&#xD;
        }&#xD;
    }&#xD;
</body>
